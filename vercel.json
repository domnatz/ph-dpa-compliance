{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/build",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/manifest.js": {
      "memory": 256,
      "maxDuration": 5
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/manifest.json", "destination": "/api/manifest.js" },
    { "source": "/(.*)", "destination": "/" }
  ]
}