{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "src": "/(.*)\\.(.+)",
      "dest": "/client/build/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
}